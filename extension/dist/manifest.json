{
   "manifest_version": 2,
   "name": "LinkedIn",
   "version": "0.1",
   "icons": {
     "16":  "icon16.png",
     "48":  "icon48.png",
     "128": "icon128.png"
   },
   "background": {
      "scripts": [
        "background.js"
      ]
   },
   "content_scripts": [
      {
         "matches": [
            "*://*.linkedin.com/*",
            "http://localhost/*"
         ],
         "js": [
            "content.js"
         ]
      }
   ],
   "browser_action": {
     "default_icon": "icon16.png"
   },
   "permissions": [
      "notifications",
		"tabs",
		"cookies",
		"webRequest",
		"webRequestBlocking",
      "http://localhost/*",
      "*://*.linkedin.com/*"
   ]
}